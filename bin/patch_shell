#!/bin/sh


if [[ "$0" != "$BASH_SOURCE" ]]; then

   #echo "patch_shell is being sourced"

   SCRIPT_PATH="$BASH_SOURCE"
   
else

   #echo "patch_shell is being run"

   SCRIPT_PATH=$0

fi


#echo "Script's file path is $SCRIPT_PATH"


SCRIPT_FOLDER=$(dirname "$SCRIPT_PATH")


$SCRIPT_FOLDER/application_build_helper --patch-shell



